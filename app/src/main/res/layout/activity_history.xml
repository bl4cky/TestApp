<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="example.testapp.HistoryActivity"
    android:background="@mipmap/logo_v5">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonProfil"
        android:background="@mipmap/salesman"
        android:minHeight="60dp"
        android:minWidth="60dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="20dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonToDo"
        android:background="@mipmap/white_list"
        android:minWidth="60dp"
        android:minHeight="60dp"
        android:layout_alignTop="@+id/imageButtonProfil"
        android:layout_toRightOf="@+id/imageButtonProfil"
        android:layout_toEndOf="@+id/imageButtonProfil"
        android:layout_marginLeft="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="History"
        android:id="@+id/textView5"
        android:layout_below="@+id/imageButtonHistory"
        android:layout_alignRight="@+id/imageButtonHistory"
        android:layout_alignEnd="@+id/imageButtonHistory"
        android:layout_marginRight="7dp"
        android:textSize="15dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonHistory"
        android:background="@mipmap/white_list_folder"
        android:minWidth="60dp"
        android:minHeight="60dp"
        android:layout_alignTop="@+id/imageButtonToDo"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:layout_marginEnd="20dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonVA"
        android:background="@mipmap/network_monitor"
        android:minHeight="60dp"
        android:minWidth="60dp"
        android:layout_alignTop="@+id/imageButtonHistory"
        android:layout_alignRight="@+id/imageButtonHistory"
        android:layout_alignEnd="@+id/imageButtonHistory"
        android:layout_marginRight="75dp"
        android:layout_marginEnd="33dp" />

</RelativeLayout>
